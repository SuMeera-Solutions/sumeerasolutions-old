    {
      "section_number": "1910.165",
      "title": "Employee alarm systems",
      "subsections": [
        {
          "subsection": "1910.165(d)",
          "title": "Maintenance and testing",
          "rules": [
            {
              "rule_id": "1910.165(d)(2)",
              "text": "The employer shall assure that a test of the reliability and adequacy of non-supervised employee alarm systems is made every two months. A different actuation device shall be used in each test of a multi-actuation device system so that no individual device is used for two consecutive tests.",
              "requirement": "test non-supervised employee alarm systems every two months using different actuation devices",
              "type": "mandatory",
              "severity": "high",
              "applies_to": "non-supervised employee alarm systems",
              "work_types": ["alarm testing", "maintenance"],
              "triggers": {
                "conditions": [
                  {"id": "test_frequency", "parameter": "testing frequency", "value": "2", "unit": "months", "operator": "<="},
                  {"id": "device_rotation", "parameter": "actuation device rotation", "value": "different device each test", "unit": "requirement", "operator": "="}
                ],
                "expression": "test_frequency AND device_rotation"
              },
              "protections": ["alarm system reliability"],
              "exceptions": {
                "items": [],
                "expression": ""
              }
            },
            {
              "rule_id": "1910.165(d)(4)",
              "text": "The employer shall assure that employee alarm circuitry installed after January 1, 1981, which is capable of being supervised is supervised and that it will provide positive notification to assigned personnel whenever a deficiency exists in the system.",
              "requirement": "supervise employee alarm circuitry installed after 1981 that is capable of supervision",
              "type": "mandatory",
              "severity": "high",
              "applies_to": "employee alarm circuitry installed after January 1, 1981",
              "work_types": ["alarm supervision", "system monitoring"],
              "triggers": {
